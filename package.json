{
  "name": "react-parcel-tailwind",
  "version": "1.2.0",
  "private": true,
  "description": "Boilerplate React-Parcel-TailwindCSS",
  "scripts": {
    "parcel:serve": "parcel serve src/index.html -p 1234 --open",
    "parcel:build": "parcel build src/index.html --public-url ./ --dist-dir dist --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean lint stylelint parcel:serve",
    "build": "npm-run-all clean lint stylelint parcel:build",
    "stylelint": "stylelint src/**/*.scss",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansango/react-parcel-tailwindcss-bp"
  },
  "author": {
    "name": "Anibal Santos",
    "email": "anibalsantosgo@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ansango/react-parcel-tailwindcss-bp/issues"
  },
  "homepage": "https://github.com/ansango/react-parcel-tailwindcss-bp#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@parcel/transformer-sass": "^2.0.0-nightly.652",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "prettier": "^2.2.1",
    "autoprefixer": "^10.2.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-nightly.566",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.5",
    "sharp": "^0.27.1",
    "tailwindcss": "^2.1.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
